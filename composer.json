{
	"name": "chillerlan/php-httpinterface",
	"description": "A http PSR-7/17 client/interface for PHP7.2+",
	"license": "MIT",
	"type": "library",
	"keywords": [
		"http", "client", "psr-7", "psr-17"
	],
	"minimum-stability": "stable",
	"authors": [
		{
			"name": "smiley",
			"email": "smiley@chillerlan.net",
			"homepage": "https://github.com/codemasher"
		}
	],
	"support": {
		"issues": "https://github.com/chillerlan/php-httpinterface/issues",
		"source": "https://github.com/chillerlan/php-httpinterface"
	},
	"provide": {
		"psr/http-message-implementation": "1.0",
		"psr/http-factory-implementation": "1.0"
	},
	"require": {
		"php": "^7.2",
		"ext-curl":"*",
		"fig/http-message-util":"^1.1",
		"php-http/httplug":"^1.1",
		"psr/http-message": "^1.0",
		"psr/http-factory":"^1.0",
		"psr/log": "^1.0"
	},
	"require-dev": {
		"guzzlehttp/guzzle": "^6.3",
		"guzzlehttp/psr7": "^1.4",
		"phpunit/phpunit": "^7.3"
	},
	"autoload": {
		"psr-4": {
			"chillerlan\\HTTP\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"chillerlan\\HTTPTest\\": "tests/",
			"chillerlan\\HTTPExamples\\": "examples/"
		}
	}
}
